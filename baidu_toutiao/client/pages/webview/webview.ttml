<view >
    <web-view  tt:if="{{src}}" src="{{src}}"></web-view>
    <view  class="tip">web-view是承载网页的容器</view>
    <swan-button  type="primary" class="btn" bindtap="openWebViewInNewPage">新页面打开web-view</swan-button>
    <swan-button  type="primary" class="btn" bindtap="openWebViewCurrent">当前页打开web-view</swan-button>
</view>